#!/bin/bash

RUN="caws run "
LS="ls -lag "
FILTER_IN="caws setfltrinchs "
FILTER_OUT="caws setfltroutchs "
FILTER="caws genfltr "
CHANNEL="caws gench "
FSET="caws setfltrpar "

env=`uname`
cpu=`uname -p`

if [ $cpu = "x86_64" ]; then
    export path_aws=/home/ubuntu/matumoto/aws
    export path_data=/home/ubuntu/matumoto/aws
else
    export path_aws=/mnt/aws
    export path_data=/mnt/aws
fi
